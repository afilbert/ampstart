{
  "land-see": {
    "head-tag": {
      "title": "Land + See",
      "canonical-path": "templates/land-see/land-see.amp",
      "extensions": [
        "amp-carousel",
        "amp-sidebar",
        "amp-accordion",
        "amp-instagram",
        "amp-fx-parallax",
        "amp-animation",
        "amp-position-observer"
      ],
      "css-path": "templates/land-see/page.css",
      "font-stylesheets": [
        "https://fonts.googleapis.com/css?family=Inconsolata|Montserrat:400,500,600,700|Crimson+Text",
        "https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css"
      ]
    }
  },
  "navbar": {
    "sidebar-id": "header-sidebar",
    "logo": {
      "name": "Land + See"
    },
    "nav": [
      {
        "link": {"url": "stories.amp.html", "nav-item-classes": "sidebar-nav-item", "text": "Stories"},
        "contains-sub-nav": true,
        "sub-nav": [
          {
            "sub-link": {"sub-url": "#", "sub-text": "All"}
          },
          {
            "sub-link": {"sub-url": "#", "sub-text": "Design"}
          },
          {
            "sub-link": {"sub-url": "#", "sub-text": "Fashion"}
          },
          {
            "sub-link": {"sub-url": "#", "sub-text": "Interiors"}
          },
          {
            "sub-link": {"sub-url": "#", "sub-text": "Arts & Culture"}
          },
          {
            "sub-link": {"sub-url": "#", "sub-text": "Popular"}
          }
        ]
      },
      {
        "link": {"url": "#", "nav-item-classes": "sidebar-nav-item", "text": "Shop"}
      },
      {
        "link": {"url": "#", "nav-item-classes": "sidebar-nav-item", "text": "About"}
      },
      {
        "link": {"url": "#", "nav-item-classes": "sidebar-nav-item", "text": "Contact"}
      },
      {
        "search": {"text": "Search"}
      }
    ],
    "social-follow": {
      "links": [
        {"twitter": "#"},
        {"facebook": "#"},
        {"instagram": "#"},
        {"pinterest": "#"},
        {"email": "#"}
      ]
    }
  },
  "accordion": {
    "section": [
      {
        "title": "Arts & Culture", "classes": "accordion-bg-30", "expanded": "expanded",
        "land-see-post": {
          "image": [
            {"classes": "mb3 cover", "alt-text": "Good eats.", "width": 196, "height": 249,
             "layout": "responsive", "src": "../../img/land-see/content/burger-200px.jpg"}
          ],
          "post-category": "Arts & Culture",
          "post-title": "Quick Bites in the Bay Area",
          "post-copy": "From Sonoma to Santa Clara, we've found the best spots for quick and delicious bites."
        }
      },
      {"title": "Design", "classes": "accordion-bg-54"},
      {"title": "Fashion", "classes": "accordion-bg-77"},
      {"title": "Interiors", "classes": "accordion-bg-100"}
    ]
  },
  "instagram-cta": {
    "url": "https://www.instagram.com/lifeatgoogle/",
    "image": [
      {"classes": "mt3 mb3", "alt-text": "Instagram, follow us!", "media-query": "(min-width: 608px)",
       "width": 813, "height": 270, "layout": "responsive",
       "src": "../../img/land-see/content/instagram-cta-lg.png"},
      {"classes": "mt3 mb3", "alt-text": "Instagram, follow us!", "media-query": "(min-width: 361px) and (max-width: 607px)",
       "width": 607, "height": 282, "layout": "responsive",
       "src": "../../img/land-see/content/instagram-cta-md.png"},
      {"classes": "mt4 mb4", "alt-text": "Instagram, follow us!", "media-query": "(max-width: 360px)",
       "width": 360, "height": 210, "layout": "responsive",
       "src": "../../img/land-see/content/instagram-cta-sm.png"}
    ]
  },
  "subscription": {
    "form": {
      "title": "Subscribe to Land + See",
      "method": "GET",
      "action": "/",
      "heading": {
        "level": 2,
        "classes": "subscription-heading-text",
        "text": "Weekly inspiration from "
      },
      "inputs": [
        {
          "input": {
            "type": "email",
            "value": "",
            "placeholder": "SUBSCRIBE",
            "id": "email",
            "name": "name2"
          }
        },
        {
          "input": {
            "type": "submit",
            "value": "",
            "id": "submit",
            "name": "submit"
          }
        }
      ]
    }
  },
